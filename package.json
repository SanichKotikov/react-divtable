{
  "name": "react-divtable",
  "version": "1.2.0",
  "description": "A simple sortable table (div) component for react",
  "main": "./lib/index",
  "jsnext:main": "./es6/index",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "styles.scss"
  ],
  "scripts": {
    "build": "npm run lint && npm run build-cjs && npm run build-es6",
    "build-cjs": "rm -rf lib && mkdir lib && babel --presets es2015,stage-0,react ./modules -d lib",
    "build-es6": "rm -rf es6 && mkdir es6 && babel --presets stage-0,react ./modules -d es6",
    "examples": "webpack -w",
    "lint": "eslint ./modules/*.js ./examples/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SanichKotikov/react-divtable.git"
  },
  "author": "Sanich <sancho@cdrpro.ru> (http://cdrpro.ru)",
  "license": "MIT",
  "keywords": [
    "table",
    "sortable",
    "sortable-table",
    "react-component",
    "react",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/SanichKotikov/react-divtable/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.15.1",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-loader": "^0.8.2",
    "postcss-nesting": "^2.3.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sort-array-objects": "2.0.0"
  },
  "homepage": "https://github.com/SanichKotikov/react-divtable"
}
