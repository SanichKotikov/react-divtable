{
  "name": "react-divtable",
  "version": "1.0.0",
  "description": "A simple sortable table (div) component for react",
  "main": "./lib/index",
  "jsnext:main": "./es6/index",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "styles.scss"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rm -rf lib && mkdir lib && ./node_modules/.bin/babel --presets es2015,stage-0,react ./modules -d lib",
    "build-es6": "rm -rf es6 && mkdir es6 && ./node_modules/.bin/babel --presets stage-0,react ./modules -d es6",
    "examples": "./node_modules/.bin/webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdrpro/react-divtable.git"
  },
  "author": "Sanich <sancho@cdrpro.ru> (http://cdrpro.ru)",
  "license": "MIT",
  "keywords": [
    "table",
    "sortable",
    "sortable-table",
    "react-component",
    "react",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/cdrpro/react-divtable/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.15.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "homepage": "https://github.com/cdrpro/react-divtable"
}
